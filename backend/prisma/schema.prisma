// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
model Organizacion {
    id              Int      @id @default(autoincrement())
    nombre          String
    descripcion     String
    direccion       String
    imagen          String
    aprobado        Boolean
}
model Noticia {
    id              Int      @id @default(autoincrement())
    titulo          String
    descripcion     String
    url             String
    imagen          String
    fecha           DateTime @default(now())
}